/*---------- BASE ----------*/

QWidget {
    background-color: rgb(35, 35, 35);
}

QSplitter::handle {
    background-color: rgb(50, 50, 50);
    border: 1px ridge rgb(35, 35, 35);
}

/*---------- TAB WIDGETS ----------*/

QTabWidget {
    background-color: #ff3b3b3f;
    color: rgb(200, 200, 200);
    
}
QTabWidget::pane {
    background-color: rgb(50, 50, 50);
    border: 1px ridge rgb(50, 50, 50);
    padding: 3px;
}
QWidget#generator_tab {
    background-color: #ff3b3b3f;
}
QWidget#discriminator_tab {
    background-color: #ff3b3b3f;
}
QWidget#loss_tab {
    background-color: #ff3b3b3f;
}
QWidget#saving_tab {
    background-color: #ff3b3b3f;
}
QWidget#visdom_tab {
    background-color: #ff3b3b3f;
}


QTabBar {
    background-color: rgb(50, 50, 50);
    color: rgb(200, 200, 200);
}
QTabBar::tab {
    background-color: rgb(50, 50, 50);
    border: 1px solid rgb(50, 50, 50);
    padding: 5px;
}
QTabBar::tab:selected {
    background-color: rgb(60, 60, 60);
    border: 1px ridge rgb(50, 50, 50);
}


