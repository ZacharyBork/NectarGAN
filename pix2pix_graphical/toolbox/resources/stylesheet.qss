    border: 1px solid #ff3b3b3f;background-color: #ff3b3b3f;border-top-right-radius: 10px;}

QWidget#utilities_page {
    border: 1px solid #ff3b3b3f; background-color: #ff3b3b3f; border-top-right-radius: 10px;}
QWidget#utilities_settings_container {
