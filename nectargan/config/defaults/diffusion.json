                    "features": 256,
                    "n_downs": 5
                }
            },
            "latent": {
                "input_size": 256,
                "latent_size_divisor": 8,
                "override_latent_size": false,
                "latent_size": 32,
                "precache": {
                    "enable": true,
                    "batch_size": 64,
                    "shard_size": 512
                },
                "dae": {
                    "in_channels": 4,
                    "features": 256,
                    "n_downs": 2
                }
            }
        },
        "save": {
            "save_model": true,
            "model_save_rate": 1,
            "auto_increment_version": true,
            "save_examples": true,
            "example_save_rate": 200,
